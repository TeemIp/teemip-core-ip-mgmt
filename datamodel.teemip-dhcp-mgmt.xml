    <class id="PhysicalDevice" _created_in="itop-config-mgmt" _delta="must_exist">
      <fields>
        <field id="dhcpoptionhosts_list" xsi:type="AttributeLinkedSet" _delta="define">
          <linked_class>DHCPOptionHost</linked_class>
          <ext_key_to_me>physicaldevice_id</ext_key_to_me>
          <tracking_level/>
          <edit_mode/>
        </field>
      </fields>
      <methods>
        <method id="DisplayBareRelations" _delta="define">
          <comment/>
          <static>false</static>
          <access>public</access>
          <type>Custom</type>
          <code><![CDATA[ public function DisplayBareRelations(WebPage $oP, $bEditMode = false)
    {
        // Execute parent function first
        parent::DisplayBareRelations($oP, $bEditMode);

        if (!$bEditMode)
        {
          $aProfiles = UserRights::ListProfiles();
          if (in_array('DHCP Manager', $aProfiles) || in_array('Administrator', $aProfiles))
          {
              // Search for host DHCP options
              $sOQL = "SELECT DHCPOptionHost WHERE physicaldevice_id = :id";
              $oOptionSet =  new CMDBObjectSet(DBObjectSearch::FromOQL($sOQL), array(), array('id' => $this->GetKey()));
              if ($oOptionSet->CountExceeds(0))
              {
                  $iOptions = $oOptionSet->Count();
                  $oP->SetCurrentTab(Dict::S('Class:PhysicalDevice/Tab:dhcpoptionhosts_list').' ('.$iOptions.')');
                  $oP->p(MetaModel::GetClassIcon('DHCPOptionHost').'&nbsp;'.Dict::Format('Class:PhysicalDevice/Tab:dhcpoptionhosts_list+'));
                  self::DisplaySet($oP, $oOptionSet, array('menu' => false));
              }
              else
              {
                  $oP->SetCurrentTab(Dict::S('Class:PhysicalDevice/Tab:dhcpoptionhosts_list'));
                  $oP->p(MetaModel::GetClassIcon('DHCPOptionHost').'&nbsp;'.Dict::S('UI:NoObjectToDisplay'));
              }
          }
        }
    }]]></code>
        </method>
      </methods>
    </class>
