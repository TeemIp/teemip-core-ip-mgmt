      <methods>
        <method id="ComputeFqdn">
          <static>true</static>
          <access>public</access>
          <type>Internal</type>
          <code><![CDATA[ public static function ComputeFqdn($sName, $sZoneName)
	{
		// Note: $sZoneName attribute must be fully qualified

		// Add dot
		$sName .= substr($sName, -1) != '.' ? '.' : null;
		// Add zone name
		$l = strlen($sZoneName);
		$sName .= substr_compare($sName, $sZoneName, -$l, $l) != 0 ? $sZoneName : null;

		return $sName;
	}]]></code>
        </method>
